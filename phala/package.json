{
  "name": "ai-agent-anthropic-template",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "docker run --rm --name wapo -p 8000:80 -v $(pwd)/dist:/home/wapo phalanetwork/wapo-devserver:latest",
    "build": "phat-fn build --experimentalAsync",
    "test": "tsx tests/test.ts",
    "set-secrets": "tsx scripts/setSecrets.ts",
    "lint": "tsc --noEmit",
    "publish-agent": "phat-fn build --experimentalAsync && tsx scripts/publish.ts",
    "dev": "concurrently \"npm:watch\" \"npm:start\"",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@phala/fn": "^0.2.22",
    "blakejs": "^1.2.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "js-sha3": "^0.9.3",
    "thirdweb": "^5.32.3",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "wyhash": "^1.0.0"
  },
  "dependencies": {
    "concurrently": "^9.0.1",
    "npm-watch": "^0.13.0"
  }
}
